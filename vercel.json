{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.10"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "functions": {
    "app.py": {
      "runtime": "python3.10"
    }
  },
  "crons": [
    {
      "path": "/tasks/dispatch_daily_summaries",
      "schedule": "45 16 * * *"
    },
    {
      "path": "/tasks/dispatch_profile_builds",
      "schedule": "0 18 * * *"
    }
  ]
}